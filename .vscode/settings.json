{
  "dart.flutterTestAdditionalArgs": ["--no-sound-null-safety"],
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true,
		"**/.dart_tool": true,
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true,
		"**/.dart_tool": true,
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.fvm": true,
    "**/**.mocks.dart": true,
    "**/**.freezed.dart": true,
    "**/*.g.dart": true,
    "android": true,
    "ios": false,
    "web": true,
    ".idea": true,
    "analysis_options.yaml": true,
    "**/.dart_tool": true,
    ".scannerwork": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    ".metadata": true,
    "flutter_leal_app.iml": true,
    "l10n.yaml": true,
    "sonar-project.properties": true,
    "amplify": true,
    "codesigndoc_exports": true,
    "coverage": false,
    "generated": true,
    ".env.dev": true,
    ".env.qa": true,
    ".env.prod": true,
    "build": true,
    "linux": true,
    "windows": true,
    "macos": true,
    "*.iml": true,
    "pubspec.lock": true,
    "distribution": true,
    ".githooks": true,
    "**/amplifyconfiguration.dart": true,
	},
	"cSpell.words": [
		"christmas"
	],
}